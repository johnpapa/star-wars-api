{
    "$schema": "http://json.schemastore.org/proxies",
    "proxies": {
        "swagger": {
            "matchCondition": {
                "route": "swagger.json"
            },
            "backendUri": "https://papa-starwars-api.azurewebsites.net/admin/host/swagger?code=muT8yvuJUGGzO/1yiNsSPQasde3BNMVOswzHV3RQfrcAfqkbT2atZA=="
        },
        "swagger-landing-docs": {
            "matchCondition": {
                "route": "docs"
            },
            "backendUri": "http://starwars-api-docs.azurewebsites.net"
        },
        "swagger-landing-docs-rest": {
            "matchCondition": {
                "route": "docs/{*rest}"
            },
            "backendUri": "http://starwars-api-docs.azurewebsites.net/{rest}"
        }
    }
}
